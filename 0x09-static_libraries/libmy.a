/**
 * _strpbrk - searches a string for any of a set of bytes
 * @s: string to check
 * @accept: string
 * Return: first occurrence of any type of byte contained in s
 */
char *_strpbrk(char *s, char *accept)
{
	int i = 0,  j = 0;

	for (i = 0 ; s[i] != '\0' ; i++)
	{
		for (j = 0 ; accept[j] != '\0' ; j++)
		{
			if (s[i] == accept[j])
				return (&s[i]);
		}
	}

	return (NULL);

}
